{
    "java.project.referencedLibraries": [
        "lib/**/*.jar"
    ],
    "java.project.sourcePaths": [
        "src"
    ],
    "java.project.outputPath": "out",
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.project.resourceFilters": [
        "node_modules",
        ".git"
    ],
    "java.compile.nullAnalysis.mode": "disabled",
    "java.jdt.ls.vmargs": "-XX:+UseG1GC -Xmx4G --module-path lib --add-modules javafx.controls,javafx.fxml,javafx.graphics,javafx.base --add-exports javafx.graphics/com.sun.javafx.sg.prism=ALL-UNNAMED --add-exports javafx.base/com.sun.javafx.event=ALL-UNNAMED --add-opens javafx.graphics/javafx.scene=ALL-UNNAMED --add-opens javafx.controls/javafx.scene.control=ALL-UNNAMED --add-opens javafx.graphics/javafx.scene.text=ALL-UNNAMED --add-opens javafx.graphics/javafx.geometry=ALL-UNNAMED --add-opens javafx.graphics/javafx.scene.paint=ALL-UNNAMED --add-opens javafx.graphics/javafx.scene.shape=ALL-UNNAMED --add-opens javafx.graphics/javafx.application=ALL-UNNAMED",
    "java.settings.url": "",
    "java.import.generatesMetadataFilesAtProjectRoot": false
}
